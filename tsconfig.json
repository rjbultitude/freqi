{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "noImplicitAny": false,
    "removeComments": false,
    "experimentalDecorators": true,
    "noLib": false,
    "outDir": "./lib",
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
  },
  "include" : [
      "./src/freqi.ts"
    ],
  "exclude": [
    "dist",
    "lib",
    "node_modules",
    "test",
  ]
}
