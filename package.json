{
  "name": "freqi",
  "version": "1.0.4",
  "description": "A library that generates musical frequencies for use with the Web Audio API",
  "main": "freqi.js",
  "devDependencies": {
    "@types/node": "^12.12.14",
    "browserify": "^16.5.1",
    "chai": "^4.2.0",
    "connect": "^3.7.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.7.6",
    "rollup-plugin-typescript": "^1.0.1",
    "serve-static": "^1.14.1",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "build": "rollup --config",
    "build:ts": "tsc",
    "build:demo": "browserify demo/app.js > demo/freqi-demo.js",
    "start": "cross-env NODE_ENV=dev && node server.js",
    "test": "npm-run-all test:unit",
    "test:unit": "cross-env NODE_ENV=test mocha --env.dev --opts ./mocha.opts \"./test/unit/**/*.js\"",
    "lint": "eslint lib/*.js",
    "lint:fix": "eslint --fix lib/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rjbultitude/freqi"
  },
  "keywords": [
    "music",
    "scales",
    "webaudio",
    "frequencies",
    "tones",
    "notes"
  ],
  "author": "Richard Bultitude <richard.bultitude@gmail.com>",
  "contributors": [
    "Richard Bultitude"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=4.2.4"
  },
  "bugs": {
    "url": ""
  },
  "homepage": ""
}
